package core;
class abc{
	public static void main(String[] args) {
		System.out.println("hello");
	}
}
public class FIrstDemo {
	public static void main(String[] args) {
		System.out.print("hello java\t");
		System.out.println("hello java");
		System.out.println("hello");
		int i=1;
		i--;
		System.out.println(i);
		int a = 1;
		int b = a++;
		System.out.println(b);
		int c = a++;
		System.out.println(c);
		int j = 1;
		j += a;
		System.out.println();
	}
}

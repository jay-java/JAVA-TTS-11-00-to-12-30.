package core;

import java.util.HashMap;
import java.util.Map;

public class P026_Map {
	public static void main(String[] args) {
		Map<Integer,Stirng> map = new HashMap();
		map.put(1, "c");//entry
		map.put(2, "c++");
		map.put("java", 3);
		map.put(1.4, "php");
		map.put(true, "python");
		map.put("java","java");
		System.out.println(map);
	}
}

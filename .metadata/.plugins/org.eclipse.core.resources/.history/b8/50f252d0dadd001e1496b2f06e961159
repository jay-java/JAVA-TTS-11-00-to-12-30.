package core;
class overload{
	public int add(int a, int b) {
		return a+b;
	}
	public int add(int a, int b, int c) {
		return a+b+c;
	}
	public int add(int a, int b,int c, int d) {
		return a+b+c+d;
	}
	public double add(float a, double b) {
		return a+b;
	}
}
public class P011_Polymorphism {
	public static void main(String[] args) {
		
	}
}

package core;
class A1{
	public void classA() {
		System.out.println("class A");
	}
}
class B extends A1{
	public void classB() {
		System.out.println("class B");
	}
}
class C extends B{
	public void classC() {
		System.out.println("class C");
	}
}
class D{
	
}
public class P010_Inheritance {
	public static void main(String[] args) {
		C c = new C();
		c.classA();
		c.classB();
		c.classC();
	}
}
